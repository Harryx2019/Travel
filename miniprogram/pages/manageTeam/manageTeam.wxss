page {
  background-color: rgb(246, 247, 249);
}

.navigate{
  width: 100%;
  position: fixed;
  z-index: 100;
  top: 0;
}
.navigateTitle{
  font-size: 40rpx;
  font-weight: bold;
  width: 100%;
  text-align: center;
  position: absolute;
  z-index: 10;
}
.back{
  position: absolute;
  z-index: 100;
  width: 40rpx;
  height: 40rpx;
  left: 5%;
  top: 60rpx;
}

.cover{
  width: 100%;
}

.userImg{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin-top: -50rpx;
  position: absolute;
  left: 50%;
}

.teamTitle{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.teamTitle .headImg{
  margin-top: -80rpx;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.teamName{
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 10rpx;
}
.teamSchool{
  font-size: 26rpx;
  background-color: rgb(126, 193, 230);
  color: white;
  border-radius: 25rpx;
  padding: 5rpx 10rpx;
  line-height: 40rpx;
  text-align: center;
  margin-top: 10rpx;
}
.editTeam{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  margin-top: 10rpx;
  font-weight: bold;
}
.edit{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.applyList{
  background-color: white;
  width: 90%;
  margin: 20rpx auto;
  padding: 20rpx 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  border-radius: 50rpx;
}
.empty{
  width: 100%;
  text-align: center;
}
.applyList .title{
  color: rgb(126, 193, 230);
  font-weight: bold;
}
.applyItem{
  display: flex;
  flex-direction: row;
  margin: 20rpx 0;
  padding-bottom: 10rpx;
  border-bottom: 1px solid rgb(246, 247, 249);
  position: relative;
}
.applyStatus{
  position: absolute;
  right: 5%;
  top: 20rpx;
  background-color: rgb(126,193,230);
  color: white;
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
  font-size: 26rpx;
}
.button{
  position: absolute;
  right: 10%;
  top:0;
  width: 100rpx;
  height: 50rpx;
  background-color: rgb(126, 193, 230);
  text-align: center;
  font-size: 24rpx;
  line-height: 50rpx;
  font-weight: bold;
  color: white;
  border-radius: 25rpx;
}
.applyItem .applicant{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.applyInfo{
  display: flex;
  flex-direction: column;
  width: 80%;
  font-size: 26rpx;
}
.applyInfo .top{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.applyInfo .top .name{
  font-size: 32rpx;
}
.applyInfo .top .sex{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}
.applyInfo .top .location{
  margin-left: 20rpx;
  font-weight: normal;
  background-color: beige;
  padding: 5rpx 10rpx;
  border-radius: 25rpx;
}
.school,.institute,.grade,.description,.phone,.qq{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 5rpx 0;
}
.itemTitle{
  color: rgb(126, 193, 230);
  margin-right: 10rpx;
}
.itemIcon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.addMember{
  background-color: rgb(0,0,0,0.5);
  width: 100%;
  position: fixed;
  top: 0;
}
.add{
  position: absolute;
  width: 80%;
  height: 280rpx;
  left: 10%;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 50rpx;
  padding: 20rpx 10rpx;
}
.cancel{
  position: absolute;
  top: 0;
  right: 0;
  width: 50rpx;
  height: 50rpx;
}
.addTitle{
  width: 100%;
  text-align: center;
  color: rgb(126, 193, 230);
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.search{
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 70%;
  border-radius: 50rpx;
  border: 1px solid rgb(246, 247, 249);
  padding: 5rpx 10rpx;
  text-align: center;
}
.searchIcon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.searchMember{
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: wrap;
  margin: 20rpx 0;
}
.memberItem{
  width: 50%;
  display: flex;
  flex-direction: row;
  align-items: center;
  border: 1px solid rgb(230,230,230);
  border-radius: 50rpx;
}
.memberItem:hover{
  background-color: rgb(126, 193, 230);
  color: white;
}
.searchMemberImg{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.searchMemberInfo{
  width: 60%;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
}
.searchSchool{
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:1; 
  -webkit-box-orient:vertical;
}