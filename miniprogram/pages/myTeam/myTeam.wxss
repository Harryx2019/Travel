page{
  background-color: rgb(246,247,249);
}
.cover{
  width: 100%;
}

.navigate{
  width: 100%;
  position: fixed;
  top: 0;
}
.navigateTitle{
  font-size: 40rpx;
  font-weight: bold;
  width: 100%;
  text-align: center;
  position: absolute;
  z-index: 10;
}
.back{
  position: absolute;
  z-index: 100;
  width: 40rpx;
  height: 40rpx;
  left: 5%;
  top: 60rpx;
}

.userImg{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin-top: -50rpx;
  position: absolute;
  left: 50%;
}

.team{
  width: 90%;
  margin: 0 auto;
  background-color: white;
  border-radius: 25rpx;
  margin-top: 100rpx;
  position: relative;
  overflow: hidden;
}
.tag{
  position: absolute;
  right: -10rpx;
  top:30rpx;
  padding: 10rpx 20rpx;
  border-radius: 25rpx;
  font-size: 24rpx;
}
.empty{
  display: block;
  width: 100%;
  text-align: center;
  padding: 20rpx 0;
  color:grey;
}
.teamType{
  color: rgb(126,193,230);
  font-size: 36rpx;
  font-weight: bold;
  display: block;
  padding-top: 20rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
}
.teamItem{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.teamHeadImg{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.teamInfo{
  width: 80%;
  height: 580rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-bottom: 20rpx;
}
.teamInfo .teamName{
  font-weight: bold;
}
.teamInfo .teamSchool{
  font-size: 26rpx;
  background-color: rgb(245,245,245);
  border-radius: 25rpx;
  padding: 5rpx 10rpx;
  line-height: 40rpx;
  max-width: 300rpx;
  text-align: center;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:1; 
  -webkit-box-orient:vertical;
}
.teamInfo .teamData{
  width: 80%; 
  height: 200rpx;
  background-color: beige;
  border-radius: 25rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-left: 50rpx;
}
.teamData .dataItem{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
}
.dataIcon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.teamDes{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.teamDes .title{
  font-size: 30rpx;
  color: rgb(126,193,230);
}
.teamDes .context{
  width: 70%;
  font-size: 26rpx;
  margin-left: 10rpx;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2; 
  -webkit-box-orient:vertical;
}
.teamMember{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.teamMember .title{
  margin-right: 10rpx;
}
.teamMember .memberList{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 24rpx;
  margin: 0 10rpx;
}
.teamMember .memberList .memberName{
  width: 100rpx;
  display:-webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1; 
  text-align: center;
}
.teamMember .memberList .memberImg{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}