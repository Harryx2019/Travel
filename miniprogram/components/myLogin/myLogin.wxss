.myLogin{
  position: absolute;
  width: 100%;
}
.user{
  position: absolute;
  padding: 0;
  border-radius: 50%;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: rgb(0,0,0,0);
  z-index: 100;
}
.userImg{
  position: absolute;
  z-index: 100;
  border-radius: 50%;
  overflow: hidden;
  width: 60rpx; 
  height: 60rpx;
}